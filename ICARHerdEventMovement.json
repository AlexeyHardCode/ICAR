{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "id": {
            "@id": "https://www.datalinker.org/context/ICARHerdEventMovement-id",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Unique Id of the herd movement event"
        },
        "event": {
            "@id": "https://www.datalinker.org/context/ICARHerdEventCore",
            "@type": "https://www.datalinker.org/contexts/2.0/ICARHerdEventCore.jsonld",
            "type": "object",
            "dltype": "https://www.datalinker.org/context/ICARHerdEventCore",
            "dldescription": "the meta data for this event"
        },
        "dairyHerdEventId": {
            "@id": "https://www.datalinker.org/context/ICARHerdEventMovement-dairyHerdEventId",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "herd event id that this movement event is part of"
        },
        "typeOfMovement": {
            "@id": "https://www.datalinker.org/context/ICARHerdEventMovement-typeOfMovement",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Type of movement either Arrival or Departure"
        },
        "date": {
            "@id": "https://www.datalinker.org/context/ICARHerdEventMovement-date",
            "@type": "xsd:dateTime",
            "type": "string",
            "dldescription": "The date the movement event occured"
        },
        "farmId": {
            "@id": "https://www.datalinker.org/context/ICARHerdEventMovement-farmId",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Unique Id of the Farm used in the movement event"
        },
        "holdingPlace": {
            "@id": "https://www.datalinker.org/context/ICARHerdEventMovement-holdingPlace",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Government Id for the barn to which the animal arrives"
        },
        "locationId": {
            "@id": "https://www.datalinker.org/context/ICARHerdEventMovement-locationId",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Identification of the herd from which the animals have been transferred from"
        },
        "ownerType": {
            "@id": "https://www.datalinker.org/context/ICARHerdEventMovement-ownerType",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "The type of the previous owner (Farm, Agent, Ministry Customer)"
        },
        "transferCause": {
            "@id": "https://www.datalinker.org/context/ICARHerdEventMovement-transferCause",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "the cause/reason for transfer of the animals"
        }
    }
}