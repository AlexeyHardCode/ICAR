{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "event": {
            "@id": "https://www.datalinker.org/context/ICARHerdEventCore",
            "@type": "https://www.datalinker.org/contexts/2.0/ICARHerdEventCore.jsonld",
            "type": "object",
            "dltype": "https://www.datalinker.org/context/ICARHerdEventCore",
            "dldescription": "the meta data for this event"
        },
        "dairyHerdEventId": {
            "@id": "https://www.datalinker.org/context/ICARHerdEventMilking-dairyHerdEventId",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "herd event id that this milking event is part of"
        },
        "milkingMode": {
            "@id": "https://www.datalinker.org/context/ICARHerdEventMilking-milkingMode",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "The method of milking (Manual or Robotic)"
        },
        "sampleBatchIds": {
            "@id": "https://www.datalinker.org/context/ICARHerdEventMilking-sampleBatchIds",
            "@container": "@set",
            "@type": "xsd:string",
            "type": "array",
            "items": {
                "type": "string"
            },
            "dldescription": "The list of samples ids that were taken during the milking event"
        },
        "sampleMethod": {
            "@id": "https://www.datalinker.org/context/ICARHerdEventMilking-sampleMethod",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "The method of sampling"
        }
    }
}