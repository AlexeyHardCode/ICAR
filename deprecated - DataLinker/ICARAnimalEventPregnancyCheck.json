{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "id": {
            "@id": "https://www.datalinker.org/context/ICARAnimalEventReproductionPregnancyCheck-id",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Unique Id of the animal event"
        },
        "event": {
            "@id": "https://www.datalinker.org/context/ICARAnimalEventCore",
            "@type": "https://www.datalinker.org/contexts/2.0/ICARAnimalEventCore.jsonld",
            "type": "object",
            "dldescription": "the meta data for this event"
        },
        "method": {
            "@id": "https://www.datalinker.org/context/ICARAnimalEventReproductionPregnancyCheck-method",
            "@type": "xsd:string",
            "type": "string",
            "enum": ["E", "P", "M", "O"],
            "dldescription": "Method used for the pregnancy check ( Echography : E , Palpation : P , Milk Anaylsis : M , Other : O)"
        },
        "result": {
            "@id": "https://www.datalinker.org/context/ICARAnimalEventReproductionPregnancyCheck-result",
            "@type": "xsd:string",
            "type": "string",
            "enum": ["U", "E", "P"],
            "dldescription": "Results of the pregnancy check ( Unknown : U , Empty : E , Pregnant : P)"
        }
    }
}