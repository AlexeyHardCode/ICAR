{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "id": {
            "@id": "https://www.datalinker.org/context/ICARAnimalEvent-id",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Unique Id of the animal event"
        },
        "MilkingEvents": {
            "@id": "https://www.datalinker.org/context/ICARAnimalEvent-MilkingEvents",
            "@type": "xsd:string",
            "type": "string",
