            "dldescription": "URL to access Milking Events for the Animal"
        },
        "ReproductionEvents": {
            "@id": "https://www.datalinker.org/context/ICARAnimalEvent-ReproductionEvents",
            "@type": "xsd:string",
            "type": "string",
            "format": "uri",
            "dldescription": "URL to access Reproduction Events for the Animal"
        },
        "MovementEvents": {
            "@id": "https://www.datalinker.org/context/ICARAnimalEvent-MovementEvents",
            "@type": "xsd:string",
            "type": "string",
            "format": "uri",
            "dldescription": "URL to access Movement Events for the Animal"
        },
        "ParturationOffspringEvent": {
            "@id": "https://www.datalinker.org/context/ICARAnimalEvent-ParturationOffspringEvent",
            "@type": "xsd:string",
            "type": "string",
