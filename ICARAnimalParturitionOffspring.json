{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "id": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-id",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Unique Id of the Parturation OffSpring event"
        },
        "event": {
            "@id": "https://www.datalinker.org/context/ICARAnimalEventCore",
            "@type": "https://www.datalinker.org/contexts/2.0/ICARAnimalEventCore.jsonld",
            "type": "object",
            "dldescription": "the meta data for this event"
        },
        "animalId": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-animalId",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Unique Id of the related animal"
        },
        "anomalias": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-anomalias",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Possible malformations/abnormalities of the calf (Yes, No, Unknown)"
        },
        "anomaliasExplanation": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-anomaliasExplanation",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Explanation of the abnormality (Not Defined, General Malformation, Head Malformed, etc(https://test.nordiccdx.com/Help/Codeset?name=CALF_ANOMALIAS_EXP))"
        },
        "birthSize": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-birthSize",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "The size of the born calf (Small, Almost average, Above average, Big)"
        },
        "birthStatus": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-birthStatus",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Status of the calf after birth (Unknown, Live calf, Abortion, Stillborn, etc(https://test.nordiccdx.com/Help/Codeset?name=CALF_BIRTH_STATUS))"
        },
        "breed": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-breed",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Breed of the calf (Unknown, Ayrshire, Holstein, BrownSwiss, Hereford, etc(https://test.nordiccdx.com/Help/Codeset?name=BREED))"
        },
        "carcassRemoval": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-carcassRemoval",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "How the dead calf was disposed of (Unknown, Burn, Carcass, Burial, Carcass Collection, Other)"
        },
        "childObjectId": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-childObjectId",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Id of inner object, unique per parent event"
        },
        "deathDate": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-deathDate",
            "@type": "xsd:dateTime",
            "type": "string",
            "dldescription": "Date of death of calf"
        },
        "earNumber": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-earNumber",
            "@type": "xsd:integer",
            "type": "string",
            "dldescription": "Ear number of the calf"
        },
        "gender": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-gender",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "The gender of the calf (Male, Female, Unknown)"
        },
        "geneticDamId": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-geneticDamId",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Id of the genetic dam (different from carrier with embryos)"
        },
        "geneticManipulation": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-geneticManipulation",
            "@type": "xsd:boolean",
            "type": "boolean",
            "dldescription": "Is the calf geneticly manipulated (True or False)"
        },
        "geneticSireId": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-geneticSireId",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Id of the Sire"
        },
        "hornStatus": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-hornStatus",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Horned state of calf (Polled, Horned, Unknown)"
        },
        "markingDate": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-markingDate",
            "@type": "xsd:dateTime",
            "type": "string",
            "dldescription": "Date of earmarking of calf"
        },
        "name": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-name",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Name of the calf"
        },
        "price": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-price",
            "@type": "xsd:integer",
            "type": "string",
            "dldescription": "Price of the calf"
        },
        "shortName": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-shortName",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Short name of the calf"
        },
        "sireBreed": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-sireBreed",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Breed of the sire (Unknown, Ayrshire, Holstein, BrownSwiss, Hereford, etc(https://test.nordiccdx.com/Help/Codeset?name=BREED))"
        },
        "sireHerdBookNumber": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-sireBreedBookNumber",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Herd Book Number of the sire"
        },
        "useMode": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-useMode",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Usage of the calf (Unknown, Dairy, Suckler, Meat production, Breeding animal, Test animal, Other)"
        },
        "weight": {
            "@id": "https://www.datalinker.org/context/ICARAnimalParturitionOffspring-weight",
            "@type": "xsd:integer",
            "type": "string",
            "dldescription": "Weight of the calf"
        }
    }
}