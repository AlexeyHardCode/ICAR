{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "id": {
            "@id": "https://www.datalinker.org/context/ICARAnimalEventMetaData-id",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Unique Id of the animal event"
        },
        "animal": {
            "@id": "https://www.datalinker.org/context/ICARMilkingAnimalCore",
            "@type": "http://www.datalinker.org/contexts/2.0/ICARMilkingAnimalCore.jsonld",
            "type": "object",
            "dldescription": "Returns ICARMilkingAnimalCore object"
        },
        "herdEvent": {
            "@id": "https://www.datalinker.org/context/ICARAnimalEventMetaData-herdEvent",
            "@type": "xsd:string",
            "type": "string",
            "format": "uri",
            "dldescription": "If this event was one of a group recorded simultaneously, the herdEvent relates the individual events"
        },
        "eventDate": {
            "@id": "https://www.datalinker.org/context/ICARAnimalEventMetaData-date",
            "@type": "xsd:dateTime",
            "type": "string",
            "dldescription": "ISO8601 date format 2018-07-19T23:00:08+00:00. The date that the event occured"
        },
        "notes": {
            "@id": "https://www.datalinker.org/context/ICARAnimalEventMetaData-notes",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Any notes relating to the event"
        },
        "reference": {
            "@id": "https://www.datalinker.org/context/ICARAnimalEventMetaData-reference",
            "@type": "xsd:string",
            "type": "string",
            "dldescription": "Unique reference for this observation within the source system.  May be an internal code"
        },
        "meta": {
            "@id": "https://www.datalinker.org/context/ICARObjectMetaData",
            "@type": "https://www.datalinker.org/contexts/2.0/ICARObjectMetaData.jsonld",
            "type": "object",
            "dldescription": "Any metadata for the event.  Most likely to use 'modified'"
        }
    }
}